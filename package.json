{
  "name": "electron-huntris",
  "productName": "Huntris built with Electron",
  "version": "0.1.0",
  "description": "Huntris built with Electron",
  "main": "main.js",
  "author": "CoderK",
  "license": "MIT",
  "scripts": {
    "electron": "node ./node_modules/.bin/electron .",
    "clean": "node ./node_modules/.bin/rimraf ./dist/**/*.*",
    "eslint": "node ./node_modules/.bin/eslint ./src/",
    "test": "node ./node_modules/.bin/karma start",
    "coverage": "node ./node_modules/.bin/rimraf ./coverage && node ./node_modules/.bin/karma start --coverage",
    "build": "node ./node_modules/.bin/webpack --progress",
    "build:watch": "node  ./node_modules/.bin/webpack --progress --watch",
    "build:production": "node ./node_modules/.bin/webpack -p --env production",
    "start": "npm run clean && npm run build && npm run electron",
    "deploy": "npm run eslint && npm run clean && npm run build:production"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.11.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.3",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^15.1.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "electron-prebuilt": "^1.2.0",
    "eventemitter3": "^1.2.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  }
}
